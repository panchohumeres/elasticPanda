upstream kibana {
    server kibana:5601;
}



server {
    listen 80;
		location / {
			proxy_pass http://kibana;
			proxy_set_header Host $host;
			proxy_set_header X-Real-IP $remote_addr;
			proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
			proxy_set_header Authorization ${KIBANA_GUEST_AUTH};
			
}
}